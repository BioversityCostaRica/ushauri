<!DOCTYPE html>
<html {% block htmlattributes %}lang="{{ request.locale_name }}" {% endblock htmlattributes %}>
{% block html %}
    <head>
        {% block head %}
            {% block meta %}
                <meta charset="utf-8">
                <meta name="viewport" content="width=device-width, initial-scale=1.0">
                <meta http-equiv="X-UA-Compatible" content="IE=edge">
                <meta name="description" content="FormShare Dashboard">
                <meta name="author" content="QLands Technology Consultants">
            {% endblock meta %}

            {% block title %}
                <title>{{ _("FormShare | Dashboard") }}</title>
            {% endblock title %}

            {% block topjs %}

            {% endblock topjs %}

            {% block css %}

            {% endblock css %}
            {% if rtl %}
                {% cssresource request,'dashboard','rtl' %}
            {% endif %}
        {% endblock head %}
    </head>

    <body {% block bodyattributes %}{% endblock bodyattributes %} class="{% if rtl %}rtls{% endif %} {% block bodyclass %}{% endblock bodyclass %}">
    {% block body %}
        {% block scripts %}

        {% endblock scripts %}
    {% endblock body %}
    </body>
{% endblock html %}
</html>
