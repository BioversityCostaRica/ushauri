{% if questions|length > 0 %}
    <h5 class="text-center">{{ _('All questions directed to') }} "{{ groupdetails.group_name }}"</h5>
    <div class="row">
        <div class="col-md-2">
            <button type="button" onclick="location.href='{{ request.route_url('dashboard', _query={'group':groupid}) }}';" id="loading-example-btn" class="btn btn-white btn-block" ><i class="fa fa-refresh"></i> {{ _('Refresh') }}</button>
        </div>
        <div class="col-md-10">

        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="table-responsive">
                <table class="table table-hover">
                    <tbody>
                    {% for question in questions %}
                        {% snippet 'dashboard/question/snippets/question_item.jinja2',request=request,activeUser=activeUser,question=question %}
                    {% endfor %}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
{% else %}
    <div class="row">
        <div class="col-md-12">
            <h5 class="text-center" style="color: red">"{{ groupdetails.group_name }}" {{ _('do not have questions') }}</h5>
        </div>
    </div>
{% endif %}

