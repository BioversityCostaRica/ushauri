{% extends 'dashboard/base.jinja2' %}

{% block css %}
    {% cssresource request,'dashboard','style' %}
{% endblock css %}

{% block bodyattributes %} style="background:#f8fafb" {% endblock bodyattributes %}

{% block body %}
    {% block bodycontent %}

    {% endblock bodycontent %}

    {% block bodybottom %}

    {% endblock bodybottom %}
    {% block scripts %}
        {% block mainscripts %}
            {% jsresource request,'dashboard','pace' %}
        {% endblock mainscripts %}
        {% block toastr %}
            {#
                Uncomment this section if you need to add a toaster
                {% snippet 'dashboard/snippets/toastr.jinja2', request=request, timeout=4000, message='A message to the user' %}
            #}
        {% endblock toastr %}
        {% block scriptsextra %}

        {% endblock scriptsextra %}
    {% endblock scripts %}
{% endblock body %}